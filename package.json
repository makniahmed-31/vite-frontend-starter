{
  "name": "vite-frontend-starter",
  "version": "1.1.6",
  "author": "github.com/makniahmed-31",
  "description": "Vite FrontEnd Starter is a boilerplate kit for easy building modern static web-sites using Vite builder",
  "keywords": [
    "vite",
    "vituum",
    "builder",
    "frontend",
    "starter",
    "boilerplate",
    "kit",
    "twig",
    "html",
    "css",
    "sass",
    "scss",
    "javascript",
    "js",
    "es6",
    "optimized",
    "autoprefixer",
    "404",
    "grid",
    "htaccess",
    "robots.txt",
    "sitemap.xml",
    "favicon"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/makniahmed-31/vite-frontend-starter"
  },
  "type": "module",
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "last 15 versions",
    "not dead"
  ],
  "lint-staged": {
    "*.js": "prettier --write"
  },
  "scripts": {
    "dev": "vite dev",
    "prod": "vite build",
    "build": "vite build",
    "preview": "vite preview",
    "docs": "vite build -- --docs",
    "lint:js": "eslint --ext .js,.jsx,.cjs,.mjs --ignore-path .gitignore ./",
    "lint:sass": "stylelint ./src/sass/**/*.{sass,scss}",
    "format": "prettier --write ."
  },
  "devDependencies": {
    "@vituum/vite-plugin-twig": "^1.1.0",
    "autoprefixer": "^10.4.17",
    "eslint": "^9.12.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-import": "^2.29.1",
    "prettier": "^3.3.3",
    "sass": "^1.79.5",
    "stylelint": "^16.2.0",
    "stylelint-config-clean-order": "^6.1.0",
    "stylelint-config-standard-scss": "^13.0.0",
    "stylelint-scss": "^6.1.0",
    "vite-plugin-minify": "^2.0.0",
    "vituum": "^1.1.1"
  },
  "dependencies": {
    "reseter.css": "^2.0.0"
  }
}
